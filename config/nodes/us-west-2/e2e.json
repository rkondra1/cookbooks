
{
  "secrets": {
    "prefix": [ "shared","wp-authoring/web/e2e" ]
  },
  "splunk": {
    "inputs": {
      "index": "cms_e2e_authoring_west2",
      "fields": {
        "appname": "wp-authoring",
        "role": "web",
        "region": "us-west-2",
        "env": "e2e"
      }
    }
  },
  "wp-authoring":{
    "wp": {
      "dbName": "wp_e2e",
      "env": "e2e"
    }
  },
  "appdynamics_machine_agent":{
    "config":{
      "application_name":"ctg_ctgu_authoring_opstest",
      "tier_name":"authoring_web_component:php_location:aws_region:us-west-2"
    }
  },
  "deploy-jms-client":{
    "jms-java-options":{
      "JAVA_OPTS": "-DENV=e2e_us-west-2"
    }
  },

  "run_list": [
    "role[preproduction]",
    "recipe[secrets]",
    "recipe[splunk]",
    "recipe[overrides::deploy_secrets]",
    "recipe[authoring::nginx]",
    "recipe[appdynamics_machine_agent::4.2.12.1]",
    "recipe[appdynamics_machine_agent::config]",
    "recipe[authoring]"
  ]
}
