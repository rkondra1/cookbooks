
{
  "secrets": {
    "prefix": [ "shared","wp-authoring/web/dev" ]
  },
  "splunk": {
    "inputs": {
      "index": "cms_dev_authoring_west2",
      "fields": {
        "appname": "wp-authoring",
        "role": "web",
        "region": "us-west-2",
        "env": "dev"
      }
    }
  },
  "wp-authoring":{
    "wp": {
      "dbName": "wp_dev",
      "env": "dev"
    }
  },
  "deploy-jms-client":{
    "jms-java-options":{
      "JAVA_OPTS": "DENV=dev_us-west-2"
    }
  },

  "run_list": [
    "recipe[secrets]",
    "recipe[splunk]",
    "recipe[overrides::deploy_secrets]",
    "recipe[nginx::1.8.0-2]",
    "recipe[authoring]",
    "recipe[authoring::nginx]",
    "recipe[authoring::php-fpm]",
    "recipe[authoring::dnsmasq]",
    "recipe[authoring::wp]",
    "recipe[authoring::plugins]",
    "recipe[authoring::appd_php]",
    "recipe[authoring::start_server]",
    "recipe[authoring::deep_healthcheck]"

  ]
}
