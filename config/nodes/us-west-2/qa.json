
{
  "secrets": {
    "prefix": [ "shared","wp-authoring/web/qa" ]
  },
  "splunk": {
    "inputs": {
      "index": "cms_qa_authoring_west2",
      "fields": {
        "appname": "wp-authoring",
        "role": "web",
        "region": "us-west-2",
        "env": "qa"
      }
    }
  },
  "wp-authoring":{
    "wp": {
      "dbName": "wp_qa",
      "env": "qa"
    },
    "appdynamics":{
      "appname":"ctg_ctgu_authoring_qa",
      "tiername":"authoring_web_component:php_location:aws_region:us-west-2"
    }

  },
  "deploy-jms-client":{
    "jms-java-options":{
      "JAVA_OPTS": "-DENV=qa_us-west-2"
    }
  },

  "run_list": [
    "recipe[secrets]",
    "recipe[splunk]",
    "recipe[overrides::deploy_secrets]",
    "recipe[nginx::1.8.0-2]",
    "recipe[authoring]"
  ]
}
