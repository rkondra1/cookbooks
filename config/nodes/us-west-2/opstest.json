{
  "secrets": {
    "prefix": [ "shared","wp-authoring/web/opstest" ]
  },
  "splunk": {
    "inputs": {
      "index": "cms_opstest_authoring_west2",
      "fields": {
        "appname": "wp-authoring",
        "role": "web",
        "region": "us-west-2",
        "env": "opstest"
      }
    }
  },
  "wp-authoring":{
    "wp": {
      "dbName": "wp_opstest",
      "env": "opstest"
    },
    "appdynamics":{
      "appname":"ctg_ctgu_authoring_opstest",
      "tiername":"authoring_web_component:php_location:aws_region:us-west-2"
    }

  },
  "deploy-jms-client":{
    "jms-java-options":{
      "JAVA_OPTS": "-DENV=opstest_us-west-2"
    }
  },

  "run_list": [
    "role[authoring]",
    "recipe[secrets]",
    "recipe[splunk]",
    "recipe[overrides::deploy_secrets]",
    "recipe[nginx::1.8.0-2]",
    "recipe[appdynamics_machine_agent::4.2.12.1]",
    "recipe[appdynamics_machine_agent::config]",
    "recipe[authoring]"
  ]
}
